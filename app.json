{
  "expo": {
    "name": "Locked-In-App",
    "slug": "Locked-In-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LockedInLogoSplash.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#F7F4F1",
      "dark": {
        "backgroundColor": "#0A364F"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alexyue.LockedIn"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LockedInLogo1024x1024.png",
        "backgroundColor": "#F7F4F1"
      },
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/LockedInLogo64x64.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "65bbf7a4-f079-48cc-8c0d-162ba4e56264"
      }
    }
  }
}
